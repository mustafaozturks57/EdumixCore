@model IEnumerable<KursMıx.Models.Program>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProgramName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProgramCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Active_Fl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DepartmanID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProgramName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProgramCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Active_Fl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DepartmanID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
